<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="TakNie" prefHeight="720.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/8.0.112"
            style="-fx-background-image: url('file:5.jpg')" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.KategoriaA">
    <children>
        <Text layoutX="230.0" layoutY="302.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text"/>
